#!/bin/bash
if [[ $# -lt 1 ]]; then
    echo "compiles .cpp file for contest fast (with optimizations, without sanitizers and additional warnings)"
    echo "usage: cfast <file>[.cpp]"
    exit 1
fi

basename=${1%.cpp}
set -o xtrace
flags=""


g++ -g3 -O3 -static -std=c++23 $flags -o $basename $basename.cpp
